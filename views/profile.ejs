<h2>Profile Page</h2>

<a href="/">Home</a> | <a href="/auth/logout">Logout</a> | <a href="/faves">Search Players</a>
<hr>
<% faves.forEach(fave => {%>
    <p><a href="/results/<%= fave.playerid %>"><%= fave.firstname %> <%= fave.lastname %> </a></p>
    <form action="/profile/<%= fave.id %>?_method=DELETE" method="POST">
        <input type="submit" value="DELETE">
        </form>
<%})%>


